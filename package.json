{
  "name": "vds-ui",
  "version": "0.7.0",
  "description": "A lightweight UI framework written in SCSS",
  "main": "./dist/vds.css",
  "filename": "vds.css",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "jest",
    "build": "gulp minify-ext minify-core minify-all",
    "build:grunt": "grunt",
    "build:sass": "sass --no-source-map --style=compressed ./src/vds.scss:dist/vds.css",
    "watch": "gulp watch",
    "prepare": "npm run snyk-protect",
    "snyk-protect": "snyk-protect"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  },
  "repository": "git+https://github.com/Victin09/vira-design-system.git",
  "keywords": [
    "vds-",
    "css",
    "UI",
    "framework"
  ],
  "author": "Víctor Gómez",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Victin09/vira-design-system/issues"
  },
  "homepage": "https://github.com/Victin09/vira-design-system",
  "autoupdate": {
    "source": "git",
    "target": "git://github.com/Victin09/vira-design-system.git",
    "basePath": "dist",
    "files": [
      "vds-.min.css",
      "vds-.css"
    ]
  },
  "devDependencies": {
    "clipboard": "^2.0.10",
    "glob": "^7.2.0",
    "global": "^4.4.0",
    "grunt": "^1.4.1",
    "gulp": "^4.0.2",
    "gulp-build": "^0.5.3",
    "gulp-clean-css": "^4.3.0",
    "gulp-cli": "^2.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-header": "^2.0.9",
    "gulp-load-plugins": "^2.0.7",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^5.1.0",
    "gulp-size": "^4.0.1",
    "jest": "^27.5.1",
    "sass": "^1.49.9",
    "sass-true": "^6.1.0"
  },
  "browserslist": [
    "last 1 major version",
    ">= 1%",
    "Chrome >= 36",
    "Firefox >= 38",
    "Edge >= 12",
    "Explorer >= 10",
    "iOS >= 9",
    "Safari >= 9",
    "Android >= 4.4",
    "Opera >= 30"
  ],
  "dependencies": {
    "@snyk/protect": "^1.707.0",
    "grunt-contrib-sass": "^2.0.0"
  },
  "snyk": true
}
